{
  "name": "selecting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "eslint": "eslint src test --ext .js,.ts,.jsx,.tsx,.json,.vue",
    "eslint:fix": "npm run lint -- --fix",
    "prettier": "prettier src test --check --ignore-unknown",
    "prettier:fix": "npm run prettier -- --write",
    "stylelint": "stylelint src/**/*.{html,vue,css,scss,js,ts,jsx,tsx}",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "npm run prettier:fix && stylelint:fix && npm run eslint:fix",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0"
  }
}
